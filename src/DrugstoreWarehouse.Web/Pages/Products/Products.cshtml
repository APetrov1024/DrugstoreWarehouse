@page
@model DrugstoreWarehouse.Web.Pages.Products.ProductsModel
@{
}

@section styles {
    <abp-style-bundle name="@DrugstoreWarehouseBundles.Styles.TabulatorExtended"></abp-style-bundle>
}
@section scripts {
    <abp-script-bundle name="@DrugstoreWarehouseBundles.Scripts.TabulatorExtended"></abp-script-bundle>
    <abp-script src="/Pages/Products/CreateUpdateProductModal.js"></abp-script>
    <abp-script src="/Pages/Products/Products.js"></abp-script>
}

@section page_header
{
    @L[LocalizerKeys.Menu.Products]
}


@section content_toolbar
{
    @if (!Model.ReadOnly)
    {
        <button type="button" class="btn btn-sm btn-primary" id="addBtn">@L[LocalizerKeys.Buttons.Add]</button>
    }
}


<abp-input asp-for="@Model.ReadOnly"></abp-input>

<div id="productsTable"></div>

